*,*::before,*::after{box-sizing:border-box}ul[class],ol[class]{padding:0}body,h1,h2,h3,h4,p,ul[class],ol[class],li,figure,figcaption,blockquote,dl,dd{margin:0}body{min-height:100vh;scroll-behavior:smooth;text-rendering:optimizeSpeed;line-height:1.5}ul[class],ol[class]{list-style:none}a:not([class]){text-decoration-skip-ink:auto}img{max-width:100%;display:block}article>*+*{margin-top:1em}input,button,textarea,select{font:inherit}@media(prefers-reduced-motion:reduce){*{animation-duration:.01ms!important;animation-iteration-count:1!important;transition-duration:.01ms!important;scroll-behavior:auto!important}}body{font:100%/1.5 Lato,sans-serif}a:link,a:visited{color:#1b1b1b;padding:6px 0}a:hover,a:focus,a:active,a.active{text-decoration:none}h1{font-size:3rem}@media all and (min-width:47.9385em){h1{font-size:4rem}}h2{font-size:2rem}@media all and (min-width:47.9385em){h2{font-size:3rem}}h3{font-size:1.5rem}@media all and (min-width:47.9385em){h3{font-size:2.2rem}}h4{font-size:1rem}@media all and (min-width:47.9385em){h4{font-size:1.2rem}}.small-text{font-size:1rem}@media all and (min-width:47.9385em){.small-text{font-size:.9rem}}.logo{display:block;background:url(/media/logo.svg)0 0/274px 41px no-repeat;width:274px;height:41px}@media all and (min-width:63.9385em){.logo{flex-basis:30%}}.girdle,ul.girdle{margin:0 auto;padding:20px;max-width:95%}@media all and (min-width:47.9385em){.girdle,ul.girdle{max-width:1000px}}@media all and (min-width:74.9375em){.girdle,ul.girdle{max-width:1400px}}.hamburger{position:absolute;top:30px;right:20px;background:url(/media/iconography/hamburger.svg)no-repeat;margin:0;width:30px;cursor:pointer}.close{background:url(/media/iconography/close.svg)no-repeat}.visually-hidden{border:0;clip:rect(0 0 0 0);height:1px;margin:-1px;overflow:hidden;padding:0;position:absolute!important;width:1px}.hidden{display:none}