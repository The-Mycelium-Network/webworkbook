{
  "name": "webworkbook",
  "version": "1.0.0",
  "description": "A curated, digital, interactive book outlining a course of study with curated notes and exercises.",
  "main": "index.js",
  "scripts": {
    "dev": "tinacms dev -c \"hugo server -D\"",
    "deploy:preview": "tinacms dev -c \"hugo --gc --minify --buildFuture -b $DEPLOY_PRIME_URL\"",
    "build": "tinacms build && hugo --gc --minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schalkneethling/webworkbook.git"
  },
  "keywords": [
    "hugo",
    "css",
    "html"
  ],
  "author": "The Mycelium Network",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/schalkneethling/webworkbook/issues"
  },
  "homepage": "https://github.com/schalkneethling/webworkbook#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.20",
    "postcss-cli": "^10.1.0"
  },
  "dependencies": {
    "@tinacms/cli": "^1.0.5",
    "tinacms": "^1.2.1"
  }
}
