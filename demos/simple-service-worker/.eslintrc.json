{
  "extends": ["eslint:recommended", "plugin:import/errors", "prettier"],
  "rules": {
    "no-console": ["error", { "allow": ["error", "info"] }]
  },
  "plugins": ["import"],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "serviceworker": true
  }
}
